{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "LearnJenkinsApp-TaskDefinition-Prod",
    "containerDefinitions": [
        {
            "name": "learnjenkinsapp",
            "image": "#REPOSITORY#/#APPNAME#:#APPVERSION#",
            "portMappings": [
                {
                    "name": "ngnix-80-tcp",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": "256",
    "executionRoleArn": "arn:aws:iam::127214196725:role/ecsTaskExecutionRole"
}
